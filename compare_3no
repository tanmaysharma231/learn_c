#include <stdio.h>
void comp(int a, int b,int c);
int main(void) {
 int a,b,c;
 printf("enter 1st number\n");
 scanf("%d",&a);
 printf("\n enter 2nd number");
 scanf("%d",&b);
 printf("\n enter 3rd number");
 scanf("%d",&c);
 comp(a,b,c);
  return 0;
}
void comp(int a, int b,int c){
if (a>b&&a>c)
 printf("%d is largest",a);
else if (b>a&&b>c)
printf("%d is largest ",b);
else 
printf("%d is largest",c);
}
